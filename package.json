{
  "name": "social-network",
  "description": "A basic social network demo app",
  "version": "0.0.0",
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "license": "MIT",
  "author": {
    "name": "Amir Abdolrazaghi",
    "email": "amir@flexverse.tech"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amirmohsen/social-network.git"
  },
  "bugs": {
    "url": "https://github.com/amirmohsen/social-network/issues"
  },
  "homepage": "https://github.com/amirmohsen/social-network#readme",
  "scripts": {
    "build-builder-dev": "babel src/Builder --out-dir dist/builder --source-maps --watch",
    "build-builder": "babel src/Builder --out-dir dist/builder",
    "dev": "node --preserve-symlinks ./build --dev",
    "build": "node --preserve-symlinks ./build",
    "start": "node --preserve-symlinks ./dist/server/server",
    "prod": "npm run build && npm start",
    "test-build": "node --preserve-symlinks ./test/test-builder",
    "test": "jest",
    "config": "node --preserve-symlinks ./config",
    "config-docker": "node --preserve-symlinks ./config --docker",
    "config-test": "node --preserve-symlinks ./config --test",
    "config-docker-test": "node --preserve-symlinks ./config --docker --test"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 8
          },
          "useBuiltIns": true
        }
      ]
    ],
    "plugins": [
      "syntax-decorators",
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-export-extensions",
      "transform-object-rest-spread",
      "transform-async-generator-functions"
    ]
  },
  "dependencies": {
    "@devexpress/dx-react-core": "1.0.0-rc.1",
    "@devexpress/dx-react-grid": "1.0.0-rc.1",
    "@devexpress/dx-react-grid-material-ui": "1.0.0-rc.1",
    "auth-header": "^0.3.1",
    "babel-polyfill": "^6",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.15.2",
    "caniuse-lite": "^1.0.30000792",
    "compression": "^1.6.2",
    "connect-mongo": "^2",
    "cors": "^2.8.3",
    "crypto-js": "^3.1.9-1",
    "csrf": "^3.0.4",
    "debounce": "^1.1.0",
    "elasticsearch": "^14.0.0",
    "es6-error": "^4.0.2",
    "express": "^4.14.0",
    "express-mongo-sanitize": "^1.3.2",
    "express-session": "^1.14.2",
    "extend": "^3.0.1",
    "fetch-cookie": "^0.7.0",
    "fs-extra": "^5.0.0",
    "helmet": "^3.4.0",
    "history": "^4",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^13.1.1",
    "joi-objectid": "^2.0.0",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "material-ui": "1.0.0-beta.29",
    "material-ui-icons": "1.0.0-beta.17",
    "moment": "^2.19.2",
    "mongodb": "^3.0.1",
    "morgan": "^1.7.0",
    "node-fetch": "^1.7.3",
    "object-path-immutable": "^0.5.2",
    "path-to-regexp": "^2",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "react": "^16",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16",
    "react-grid-system": "^3.1.0",
    "react-helmet": "^5.1.3",
