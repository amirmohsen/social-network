    "react-router-dom": "^4.0.0",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "request-ip": "^2.0.2",
    "shortid": "^2.2.8",
    "validator": "^9",
    "vhost": "^3.0.2"
